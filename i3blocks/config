[window_title]
command=~/.config/i3blocks/scripts/window_title
interval=once
signal=10

[wireless]
command=~/.config/i3blocks/scripts/wireless
interval=10

[disk]
command=~/.config/i3blocks/scripts/disk
interval=30

[memory]
command=~/.config/i3blocks/scripts/memory
interval=30

[battery]
command=~/.config/i3blocks/scripts/battery
interval=30

[tztime]
command=date '+ %Y-%m-%d  %H:%M:%S'
interval=1

[volume]
command=~/.config/i3blocks/scripts/volume
interval=once
signal=11

[backlight]
command=~/.config/i3blocks/scripts/backlight
interval=once
signal=12
